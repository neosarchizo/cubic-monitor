{
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitAny": false,
    "sourceMap": true,
    "outDir": "build/electron",
    "baseUrl": "electron",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": false,
    "skipLibCheck": true
  },
  "include": ["electron"],
  "exclude": ["node_modules"]
}
